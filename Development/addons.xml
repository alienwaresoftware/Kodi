<addon
  id="skin.alienware"
  version="1.2"
  name="Alpha UI"
  provider-name="Alienware">
  <requires>
    <import addon="xbmc.gui" version="5.3.0"/>
  </requires>
  <extension
    point="xbmc.gui.skin"
    defaultthemename="textures.xbt"
    debugging="false"
    effectslowdown="0.75">
    <res width="1280" height="720" aspect="16:9" default="true" folder="720p" />
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Alpha UI skin by Alienware</summary>
    <summary lang="en_AU">Alpha UI skin by Alienware</summary>
    <summary lang="en_NZ">Alpha UI skin by Alienware</summary>
    <summary lang="en_US">Alpha UI skin by Alienware</summary>
    <description lang="en">Alpha UI is the default skin for Kodi 14.0 and above. It is a combination of concepts from many popular skins, and attempts to embrace and integrate their good ideas into a skin that should be easy for first time Kodi users to understand and use.</description>
    <description lang="en_AU">Alpha UI is the default skin for Kodi 9.11 and above. It is a combination of concepts from many popular skins, and attempts to embrace and integrate their good ideas into a skin that should be easy for first time Kodi users to understand and use.</description>
    <description lang="en_NZ">Alpha UI is the default skin for Kodi 9.11 and above. It is a combination of concepts from many popular skins, and attempts to embrace and integrate their good ideas into a skin that should be easy for first time Kodi users to understand and use.</description>
    <description lang="en_US">Alpha UI is the default skin for Kodi 9.11 and above. It is a combination of concepts from many popular skins, and attempts to embrace and integrate their good ideas into a skin that should be easy for first time Kodi users to understand and use.</description>
    <platform>all</platform>
  </extension>
</addon>
