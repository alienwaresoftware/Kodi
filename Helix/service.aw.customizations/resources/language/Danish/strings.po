#Customizations

msgctxt "#33000"
msgid "Would you like to install The HiveMind Core Components?"
msgstr "Vil du installere HiveMind-kernekomponenterne?"

msgctxt "#33001"
msgid "These components are required to receive all the benefits of The HiveMind."
msgstr "Disse komponenter er nødvendige for at opnå alle fordelene ved HiveMind."

msgctxt "#33002"
msgid "Please wait while installing HiveMind core components"
msgstr "Vent mens der installeres HiveMind kernekomponenter"

msgctxt "#33003"
msgid "Unable to install HiveMind core components"
msgstr "Kan ikke installere HiveMind kernekomponenter"

msgctxt "#33004"
msgid "Exit Code = "
msgstr "Afslutkode = "

msgctxt "#33005"
msgid "You will need to restart your computer to complete this installation."
msgstr "Du skal genstarte din computer for at afslutte denne installationen."

msgctxt "#33006"
msgid "Would you like to restart the system now?"
msgstr "Vil du genstarte systemet nu?"

msgctxt "#33007"
msgid "A new version of The HiveMind Core Components is now available!"
msgstr "Der er en ny version af HiveMind-kernekomponenterne tilgængelig nu!"

msgctxt "#33008"
msgid "Would you like to upgrade now?"
msgstr "Vil du opgradere nu?"

msgctxt "#33009"
msgid "Current Version"
msgstr "Nuværende version"

msgctxt "#33010"
msgid "New Version"
msgstr "Ny version"

msgctxt "#33011"
msgid "The HiveMind Core Components must be installed to run this addon."
msgstr "De HiveMind Core Components skal være installeret for at køre dette addon."
